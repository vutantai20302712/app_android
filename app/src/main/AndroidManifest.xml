<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project_of_Tantai"
        tools:targetApi="31">
        <activity
            android:name=".myteam"
            android:exported="false" />
        <activity
            android:name=".ComicDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".Activity_Comic_Manager"
            android:exported="false" />
        <activity
            android:name=".your_choice"
            android:exported="false" />
        <activity
            android:name=".start_app"
            android:exported="false" />
        <activity
            android:name=".Login_screen"
            android:exported="false" />
        <activity
            android:name=".Registration_screen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>